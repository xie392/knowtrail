{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*", "src/**/*.vue"],
    "exclude": ["src/**/__tests__/*"],
    "compilerOptions": {
        "composite": true,
        "noImplicitAny": false,
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "aomao/*": ["src/aomao/*"],
            "editor/*": ["src/editor/*"]
        },
        "types": ["vite/client"]
    }
}
